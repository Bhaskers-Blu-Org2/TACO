{
	"name": "vs-mda-remote",
	"description": "Agent to remotely build, run, and debug iOS apps created using Visual Studio Tools for Apache Cordova.",
	"version": "0.2.6",
	"author": {
		"name": "Microsoft Corporation",
		"email": "vscordovatools-admin@microsoft.com"
	},
	"homepage": "http://msdn.microsoft.com/en-us/vstudio/dn722381",
	"main": "./lib/server.js",
	"bin": {
		"vs-mda-remote": "./bin/vs-mda-remote"
	},

	"preferGlobal": true,
	"dependencies": {
		"cordova": "4.0.0",
		"express": "3.4.8",
		"request": "2.36.0",
		"tar": "0.1.20",
		"fstream": "0.1.28",
		"zip-stream": "0.2.3",
		"nconf": "0.6.9",
		"q": "1.0.1",
		"elementtree": "0.1.6",
		"semver": "2.3.1",
		"rimraf": "2.2.6",
		"mkdirp": "0.3.5",
		"ncp": "0.5.1"
	},
	"devDependencies": {
		"nodeunit": "0.8.7",
		"assert": "1.1.1",
		"typescript": "1.3.0",
		"mocha": "2.0.1",
		"should":  "4.3.0"
	},

	"optionalDependencies": {
		"plist-with-patches": "0.5.1",
		"ios-sim": "2.0.1"
	},

	"scripts": {
		"test": "nodeunit test",
		"prepublish": "node_modules/typescript/bin/tsc --module commonjs lib/*.ts Scripts/typings/*/*.d.ts"
	},
	"directories": {
		"lib": "lib",
		"doc": ".",
		"test": "test",
		"example": "examples"
	},
	"readmeFilename": "README.md"
}
